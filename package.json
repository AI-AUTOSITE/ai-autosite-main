{
  "name": "ai-autosite-main",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next node_modules/.cache",
    "clean:full": "rimraf .next node_modules package-lock.json",
    "reinstall": "npm run clean:full && npm install",
    "clean-dev": "npm run clean && npm run dev",
    "check-tools": "node scripts/check-tools-diff.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "fix-all": "npm run format && npm run lint -- --fix",
    "find-console": "node scripts/find-console-logs.js",
    "remove-console": "node scripts/remove-console-logs.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
