    "@types/papaparse": "^5.3.16",
    "@types/pdfjs-dist": "^2.10.377",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "typescript": "^5.3.3",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16",
    "cssnano": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.3",
    "playwright": "^1.56.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "@next/bundle-analyzer": "14.2.3"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
