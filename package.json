    "@dnd-kit/sortable": "^8.0.0",
    "@stripe/stripe-js": "^2.4.0",
    "@vercel/kv": "^3.0.0",
    "browser-image-compression": "^2.0.2",
    "canvas": "^3.2.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.540.0",
    "mammoth": "^1.10.0",
    "marked": "^11.2.0",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "qrcode": "^1.5.4",
    "react-dropzone": "^14.2.3",
    "react-flow-renderer": "^10.3.17",
    "react-pdf": "^10.1.0",
    "stripe": "^14.25.0",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^6.0.1",
    "xlsx": "^0.18.5"
